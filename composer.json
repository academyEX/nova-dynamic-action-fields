{
    "name": "tunezilla/dynamic-action-fields",
    "description": "Dynamic Action Fields for Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "TuneZilla\\DynamicActionFields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TuneZilla\\DynamicActionFields\\DynamicFieldsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
